{
  "name": "@just-be/agents-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "agents/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build:cli": "bun run --filter @just-be/github-agent build:cli",
    "typecheck": "bun run --filter '*' typecheck"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5",
    "wrangler": "^4.67.0"
  },
  "dependencies": {
    "@just-be/github-agent": "workspace:*"
  }
}
