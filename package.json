{
  "name": "@just-be/agents",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "packageManager": "bun@1.3.9",
  "workspaces": [
    "agents/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build:cli": "bun build cli/index.ts --target=node --outdir=dist/cli",
    "typecheck": "bun run --filter '*' typecheck && tsc --noEmit -p tsconfig.cli.json"
  },
  "bin": {
    "agents": "./dist/cli/index.js"
  },
  "files": [
    "dist/cli"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.3.0",
    "typescript": "^5",
    "wrangler": "^4.67.0"
  },
  "dependencies": {
    "@just-be/github-agent": "workspace:*"
  }
}
