{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "github-app-agent",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-01",
  "compatibility_flags": ["nodejs_compat"],

  "durable_objects": {
    "bindings": [
      {
        "name": "GITHUB_AGENT",
        "class_name": "GitHubAgent"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["GitHubAgent"]
    }
  ]
}
